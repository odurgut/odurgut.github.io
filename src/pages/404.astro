---
import PageMeta from "../components/PageMeta.astro";
import Layout404 from "../layouts/Layout404.astro";
import { SITE_TITLE } from "../config";
---

<Layout404>
    <PageMeta title={`${SITE_TITLE}`} slot="meta" />
    <Fragment slot="main">
        <div class="text-8xl">404</div>
        <div class="text-2xl">NOT FOUND</div>
        <div class="mt-4"><a href="/">Back to home</a></div>
    </Fragment>
</Layout404>
